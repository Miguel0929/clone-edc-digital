= bootstrap_form_for([@delireverable_package, @delireverable]) do |f|
  = f.text_field :name, label: 'Nombre'
  = f.text_area :description, label: 'Descripción'
  = f.file_field :file, label: 'Archivo de ejemplo'
  .pull-right.m-t-30.m-b-10
    = link_to 'Cancelar', delireverable_package_delireverables_path(@delireverable_package), class: 'btn btn-danger'
    = f.submit 'Guardar información', class: 'btn btn-primary'
