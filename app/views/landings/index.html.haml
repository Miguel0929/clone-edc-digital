    / BEGIN PLUGINS
    = stylesheet_link_tag("landings/plugins/pace-theme-flash")
    = stylesheet_link_tag "landings"
    = stylesheet_link_tag("landings/plugins/swiper.min")
    / END PLUGINS
    / BEGIN PAGES CSS
    = stylesheet_link_tag("landings/pages")
    = stylesheet_link_tag("landings/pages-icons")
    / END PAGES CSS
  %body.pace-complete
    / BEGIN HEADER
    %nav.header.static{"data-pages" => "header"}
      %nav.header.relative
        .container.relative
          .pull-left
            .header-inner
              %img.logo{:alt => "", "data-src-retina" => "assets/images/logo_black_2x.png", :height => "21", :src => "#{image_path('logo-edc-digital-colores-3.png')}", :width => "152"}/
              %img.alt{:alt => "", "data-src-retina" => "assets/images/logo_white_2x.png", :height => "21", :src => "#{image_path('logo-edc-digital-colores-3.png')}", :width => "152"}/
          .visible-sm-inline.visible-xs-inline.menu-toggler.pull-right{"data-pages" => "header-toggle", "data-pages-element" => "#header"}
            .one
            .two
            .three
          .pull-right.hidden-sm.hidden-xs.col-sm-6
            .header-inner.text-right
              %p.font-montserrat.fs-13.text-master.p-t-20.inline
                +52 (55) 5351 6633
                %i.m-l-10.fa.fa-phone
      / BEGIN NESTED MENU
      %nav.header.sm-header.relative.horizontal.dark.sm-p-t-20{"data-offset-top" => "60", "data-spy" => "affix"}
        .container.relative
          #header.menu-content{"data-pages-direction" => "slideRight"}
            / BEGIN HEADER CLOSE TOGGLE FOR MOBILE
            .pull-right
              %a.text-black.link.padding-10.visible-xs-inline.visible-sm-inline.pull-right.m-t-10.m-b-10.m-r-10{"data-pages" => "header-toggle", "data-pages-element" => "#header", :href => "#"}
                %i.pg-close_line
            .pull-right.sm-block.sm-full-width.menu-login
              .header-inner
                %ul.menu.no-padding
                  %li
                    %a.solicitar-cuenta-btn{"data-text" => "Preregistro", :href => preregistro_index_path} Verifica tu cuenta
                  %li
                    %a.solicitar-cuenta-btn{"data-text" => "Sign up", :href => "https://emprendiendodesdecero.com/seguimiento/", :target => "new"} Solicitar una cuenta
                  %li
                    %a.p-r-0.btn.btn-sm.btn-bordered.btn-white.block-title.fs-12.hidden-xs{"data-text" => "Login", :href => user_session_path} Iniciar sesión
                    %a.p-r-0.visible-xs{"data-text" => "Login", :href => user_session_path} Iniciar sesión
                  %li.visible-sm.visible-xs
                    %a{"data-text" => "Contact Us", :href => "#"}
                      +52 (55) 5351 6633
                      %i.m-l-10.fa.fa-phone
      / END NESTED MENU
    / END MENU
    / END HEADER
    / BEGIN JUMBOTRON
    %section.jumbotron.demo-custom-height-2.full-width{"data-pages" => "parallax"}
      .inner.full-height
        / BEGIN SLIDER
        .swiper-container
          .swiper-wrapper
            / BEGIN SLIDE
            .swiper-slide.fit.slide1
              / BEGIN IMAGE PARRALAX
              .slider-wrapper
                .image{"data-bg-overlay" => "#3056a3", "data-overlay-opacity" => "0.3", "data-pages-bg-image" => "#{image_path('banner_1.jpg')}", "data-swiper-parallax" => "30%"}
              / END IMAGE PARRALAX
              / BEGIN CONTENT
              .content-layer
                .inner.full-height
                  .container-xs-height.full-height
                    .col-xs-height.col-middle.text-center
                      .container
                        .row
                          .col-md-8.col-md-offset-2
                            %h1.light.text-white{"data-swiper-parallax" => "-15%"}
                              Comienza la construcción
                              %br>/
                              de tu idea de negocio
                            %h5.text-white.hint-text Programa de seguimiento Emprendiendo desde Cero digital
                            %a.btn.btn-white.font-montserrat.all-caps.btn-cons.fs-12.m-t-25.hidden-xs{:href => user_session_path} Inicia sesión en tu seguimiento aquí &gt;&gt;
                            %a.btn.btn-white.font-montserrat.all-caps.btn-cons.fs-12.m-t-25.visible-xs{:href => user_session_path} Inicia sesión en tu seguimiento aquí &gt;&gt;
              / END CONTENT
            / END SLIDE
          / BEGIN ANIMATED MOUSE
          .mouse-wrapper
            .mouse
              .mouse-scroll
      / END SLIDER
    / END JUMBOTRON
    / BEGIN INTRO CONTENT
    %section#stats.p-b-40.p-t-50.sm-p-b-0
      .container
        .col-md-10.col-md-offset-1
          .text-center
            %h5.block-title.m-t-40.hint-text.m-b-0 ¿Qué es EDC Digital?
            %h1.light.m-t-5.m-b-20 Conoce Emprendiendo desde Cero digital.
            %dd.m-b-30 EDC Digital es la primera plataforma en Internet y con acompañamiento de asesores, diseñada para que los emprendedores puedan construir y validar su idea de negocio en lo comercial, digital, técnico y financiero, desde cero.
            .row
              .col-md-12.col-sm-12.text-center.mb40.mb-xs-24
                .embed-video-container.embed-responsive.embed-responsive-16by9
                  %iframe.embed-responsive-item{:src => "https://player.vimeo.com/video/188382978?badge=0&title=0&byline=0&title=0"}
    / START CONTENT SECTION
    %section.p-b-85.p-t-75.p-b-65.p-t-55.text-center
      .container
        .md-p-l-20.xs-no-padding.clearfix
          .col-sm-4.no-padding
            .p-r-40.md-pr-30.xs-no-padding.clearfix
              %img.m-b-20{:alt => "", :src => "#{image_path('Parachute.svg')}"}/
              %h6.block-title.p-b-5
                Aterriza tu idea
                %i.fa.fa-chevron-right
              %p.m-b-30 EDC Digital está diseñado para que puedas dar forma correcta a tu idea de negocio
              %p.muted.font-arial.small-text.no-padding.col-xs-6.col-xs-offset-3 Desde definir al cliente hasta tu modelo financiero.
            .visible-xs.b-b.b-grey-light.m-t-30.m-b-30
          .col-sm-4.no-padding
            .p-r-40.md-pr-30.xs-no-padding.clearfix
              %img.m-b-20{:alt => "", :src => "#{image_path('Prizemedalion.svg')}"}/
              %h6.block-title.p-b-5
                Lecciones de aprendizaje
                %i.fa.fa-chevron-right
              %p.m-b-30 En EDC Digital podrás encontrar cursos y contenido de aprendizaje para que puedas formarte como emprendedor
              %p.muted.font-arial.small-text.no-padding.col-xs-6.col-xs-offset-3 Temas comerciales, herramientas digitales, financiamiento.
            .visible-xs.b-b.b-grey-light.m-t-30.m-b-30
          .col-sm-4.no-padding
            .p-r-40.md-pr-30.xs-no-padding.clearfix
              %img.m-b-20{:alt => "", :src => "#{image_path('Umbrella.svg')}"}/
              %h6.block-title.p-b-5
                Apoyo de mentores
                %i.fa.fa-chevron-right
              %p.m-b-30 En EDC Digital tendrás mentoría de expertos a distancia, que te podrán ayudar a estructurar mejor tu idea de negocio
              %p.muted.font-arial.small-text.no-padding.col-xs-6.col-xs-offset-3 Mentores, expertos, asesores en áreas especializadas.
    / END CONTENT SECTION
    / END PRICING SECTION
    / START CONTENT SECTION
    %section.p-b-50.p-t-50.bg-master-darkest.m-t-35.sm-no-margin{"data-bg-overlay" => "black", "data-overlay-opacity" => "0.3", "data-pages" => "parallax", "data-pages-bg-image" => "#{image_path('banner_2.jpg')}"}
      .container.p-b-50.p-t-50
        .row
          .col-md-8.col-md-offset-2
            .col-xs-height.col-middle
              .text-center
                %h1.text-white.col-sm-12.p-b-10 Comienza hoy mismo a construir y validar tu idea de negocio
                %a.btn.btn-lg.btn-primary.btn-cons.btn-rounded.m-t-20{:href => user_session_path} Inicia sesión en EDC Digital aquí
                %p.fs-12.font-arial.text-white.m-t-30
                  %span.hint-text
                    En caso de que aún no tengas tu cuenta,
                  puedes solicitarla
                  = succeed "." do
                    %a{:href => "https://emprendiendodesdecero.com/seguimiento", :target => "new"} aquí
    / END CONTENT SECTION
    / START FOOTER
    %section.p-b-30.p-t-40
      .container
        .row
          .col-sm-8
            %img.logo.inline.m-r-50{:alt => "", :src => "#{image_path('logo-edc-digital-colores-3.png')}"}/
            .m-t-10
              %ul.no-style.fs-11.no-padding.font-arial
                %li.inline.no-padding
                  %a.text-master.p-r-10.b-r.b-grey{:href => "https://www.distritoemprendedor.com/", :target => "new"} Distrito Emprendedor
                %li.inline.no-padding
                  %a.hint-text.text-master.p-l-10.p-r-10.b-r.b-grey{:href => "https://www.emprendiendodesdecero.com/", :target => "new"} Emprendiendo desde Cero
                %li.inline.no-padding
                  %a.hint-text.text-master.p-l-10.p-r-10.b-r.b-grey{:href => "https://www.edc-megatendencias.com/", :target => "new"} Megatentendencias
                %li.inline.no-padding
                  %a.hint-text.text-master.p-l-10.p-r-10.xs-no-padding.xs-m-t-10{:href => "https://www.edc-ideador.com/", :target => "new"} Ideador
          .col-sm-4.text-right.font-arial.sm-text-left
            %p.fs-11.muted Copyright © 2017 DISTRITO EMPRENDEDOR. Todos los derechos reservados.
    / END FOOTER
