    %meta{:content => "website", :property => "og:type"}/
    %meta{:content => "https://www.emprendiendodesdecero.com/assets/img/VIDEO-INTRODUCTORIO-COMPILADO--(0-00-32-10).png", :property => "og:image"}/
    %meta{:content => "https://www.edcdigital.mx", :property => "og:url"}/
    %meta{:content => "Plataforma digital para la construcción y validación de tu idea de negocio, para el programa Emprendiendo desde Cero", :property => "og:description"}/
    %meta{:content => "summary_large_image", :name => "twitter:card"}/
